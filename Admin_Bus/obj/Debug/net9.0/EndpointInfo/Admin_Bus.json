            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "NoiDungBaiViet": {
        "required": [
          "noiDung"
        ],
        "type": "object",
        "properties": {
          "iD_NoiDung": {
            "type": "integer",
            "format": "int32"
          },
          "iD_BaiViet": {
            "type": "integer",
            "format": "int32"
          },
          "thuTu": {
            "type": "integer",
            "format": "int32"
          },
          "noiDung": {
            "minLength": 1,
            "type": "string"
          },
          "baiVietQuangCao": {
            "$ref": "#/components/schemas/BaiVietQuangCao"
          }
        },
        "additionalProperties": false
      },
      "Register": {
        "required": [
          "email",
          "fullName",
          "numberPhone",
          "password"
        ],
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "numberPhone": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TripHistory": {
        "required": [
          "accountId",
          "busNumber",
          "endLocation",
          "fare",
          "paymentStatus",
          "startLocation",
          "startTime"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "accountId": {
            "type": "integer",
            "format": "int32"
          },
          "busNumber": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "startLocation": {
            "maxLength": 255,
            "minLength": 0,
            "type": "string"
          },
          "endLocation": {
            "maxLength": 255,
            "minLength": 0,
            "type": "string"
          },
          "startTime": {
            "type": "string",
            "format": "date-time"
          },
          "endTime": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "duration": {
            "type": "integer",
            "format": "int32",
            "nullable": true,
            "readOnly": true
          },
          "fare": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "paymentStatus": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "account": {
            "$ref": "#/components/schemas/Account"
          }
        },
        "additionalProperties": false
      },
      "UserAdminChat": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "adminId": {
            "type": "integer",
            "format": "int32"
          },
          "title": {
            "type": "string",
